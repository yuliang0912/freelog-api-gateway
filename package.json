{
  "name": "freelog-api-gateway",
  "version": "1.0.0",
  "description": "飞致网络网关服务!",
  "private": true,
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "bluebird": "^3.7.1",
    "egg-freelog-base": "^3.1.17",
    "egg-scripts": "^2.10.0",
    "lodash": "^4.17.21",
    "midway": "^1.20.3",
    "mongoose": "^5.12.7",
    "request": "^2.88.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^14.14.44",
    "cross-env": "^6.0.0",
    "egg-ci": "^1.18.0",
    "midway-bin": "^1.20.3",
    "midway-mock": "^1.20.3",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "tslint": "^6.1.3",
    "tslint-midway-contrib": "^1.20.1",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "start": "egg-scripts start --title=freelog-api-gateway --workers=1 --framework=midway --ts",
    "stop": "egg-scripts stop --title=freelog-api-gateway",
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "clean": "midway-bin clean",
    "dev": "cross-env NODE_ENV=local port=8895 midway-bin dev --ts",
    "debug": "cross-env NODE_ENV=local midway-bin debug --ts",
    "test": "npm run lint && midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "tslint --fix -p tsconfig.json -t stylish",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "autod": "midway-bin autod"
  },
  "ci": {
    "version": "10"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-server-options": {
    "workers": 1,
    "port": 3000
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "anonymous",
  "license": "MIT"
}
